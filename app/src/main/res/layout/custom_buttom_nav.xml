<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="55dp"
    android:background="@color/white">

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view3"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="@drawable/back_bottom_nav"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_home"
        android:layout_width="74dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view_shop"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <View
        android:id="@+id/view_shop"
        android:layout_width="74dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view_home"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view_setting"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.0" />

    <View

        android:id="@+id/view_setting"
        android:layout_width="74dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toStartOf="@+id/view_shop"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view_profile"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <View

        android:id="@+id/view_profile"
        android:layout_width="74dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view_setting"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_home"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:tint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/view_home"
        app:layout_constraintEnd_toEndOf="@+id/view_home"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="@+id/view_home"
        app:layout_constraintTop_toTopOf="@+id/view_home"
        app:srcCompat="@drawable/ic_home_zori" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_shop"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:tint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/view_shop"
        app:layout_constraintEnd_toEndOf="@+id/view_shop"
        app:layout_constraintStart_toStartOf="@+id/view_shop"
        app:layout_constraintTop_toTopOf="@+id/view_shop"
        app:srcCompat="@drawable/ic_shop_zori_e" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_setting"

        android:layout_width="30dp"
        android:layout_height="30dp"
        android:tint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/view_setting"
        app:layout_constraintEnd_toEndOf="@+id/view_setting"
        app:layout_constraintStart_toStartOf="@+id/view_setting"
        app:layout_constraintTop_toTopOf="@+id/view_setting"
        app:srcCompat="@drawable/ic_settin_zori_e" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_profile"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:tint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/view_profile"
        app:layout_constraintEnd_toEndOf="@+id/view_profile"
        app:layout_constraintStart_toStartOf="@+id/view_profile"
        app:layout_constraintTop_toTopOf="@+id/view_profile"
        app:srcCompat="@drawable/ic_profile_zori_e" />

</androidx.constraintlayout.widget.ConstraintLayout>